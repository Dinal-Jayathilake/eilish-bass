[
  {
    "intent_id": "GetAccountSummary",
    "description": "Retrieve a summary or overview of a specific account",
    "examples": [
      "Can you show me a summary of customer 123456789?",
      "I need the account summary for customer 987654321",
      "What's the status of account ACC-102732184-01?"
    ],
    "slots": [
      {
        "name": "account_id",
        "type": "string",
        "required": true,
        "description": "The account identifier",
        "examples": ["123456789", "ACC-102732184-01", "987654321"]
      }
    ]
  },
  {
    "intent_id": "TransferFunds",
    "description": "Transfer money between accounts",
    "examples": [
      "Transfer $500 from checking to savings",
      "Move 1000 dollars to account 987654321",
      "Send $250 to John's account"
    ],
    "slots": [
      {
        "name": "amount",
        "type": "number",
        "required": true,
        "description": "Amount to transfer",
        "examples": ["500", "1000", "250.50"]
      },
      {
        "name": "from_account",
        "type": "string",
        "required": false,
        "description": "Source account (optional)",
        "examples": ["checking", "savings", "123456789"]
      },
      {
        "name": "to_account",
        "type": "string",
        "required": true,
        "description": "Destination account",
        "examples": ["savings", "987654321", "John's account"]
      }
    ]
  },
  {
    "intent_id": "CheckBalance",
    "description": "Check account balance",
    "examples": [
      "What's my balance?",
      "How much money do I have in checking?",
      "Show me the balance for account 123456"
    ],
    "slots": [
      {
        "name": "account_type",
        "type": "string",
        "required": false,
        "description": "Type of account (checking, savings, etc.)",
        "examples": ["checking", "savings", "credit card"]
      },
      {
        "name": "account_id",
        "type": "string",
        "required": false,
        "description": "Specific account ID if provided",
        "examples": ["123456", "ACC-789012"]
      }
    ]
  },
  {
    "intent_id": "Greeting",
    "description": "User greeting or conversation starter",
    "examples": [
      "Hello",
      "Hi there",
      "Good morning",
      "Hey"
    ],
    "slots": []
  }
